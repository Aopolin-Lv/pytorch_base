  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "def encode(sentence_list, vocab, sort_by_len=True):\n",
    "    length = len(sentence_list)\n",
    "    out_sentence_list = [[vocab.get(w, 0) for w in sent] for sent in sentence_list]\n",
    "    \n",
    "    # sort by length of sentence \n",
    "    def len_argsort(seq):\n",
    "        return sorted(out_sentence_list, key=lambda x: len(x))\n",
    "    \n",
    "    if sort_by_len:\n",
    "        out_sentence_list = len_argsort(sentence_list)\n",
    "        \n",
    "    return out_sentence_list"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "train_sent_list = encode(sentences_list, vocab)"
   ]
