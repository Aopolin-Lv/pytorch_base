  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "- 分minibatch\n",
    "- 准备数据\n",
    "- 生成"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 33,
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_minibatches(n, minibatch_size, shuffle=True):\n",
    "    # 得到按batch大小划分的index_list, 如batch_size==2，则[0, 2, 4, 6...]\n",
    "    idx_list = np.arange(0, n, minibatch_size)\n",
    "    \n",
    "    # 随机\n",
    "    if shuffle:\n",
    "        np.random.shuffle(idx_list)\n",
    "    \n",
    "    # 转成numpy类型矩阵\n",
    "    minibatches = []\n",
    "    for idx in idx_list:\n",
    "        minibatches.append(np.arange(idx, min(idx + minibatch_size, n)))\n",
    "    return minibatches"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 34,
   "metadata": {},
   "outputs": [],
   "source": [
    "def fill_data(sentences):\n",
    "    lengths = [len(sen) for sen in sentences]\n",
    "    n_samples = len(sentences)\n",
    "    max_len = np.max(lengths)\n",
    "    \n",
    "    x = np.zeros((n_samples, max_len)).astype(\"int32\")\n",
    "    x_lengths = np.array(lengths).astype(\"int32\")\n",
    "    for idx, sent in enumerate(sentences):\n",
    "        x[idx, :lengths[idx]] = sent\n",
    "    return x, x_lengths"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 35,
   "metadata": {},
   "outputs": [],
   "source": [
    "def generate(train_sentence_list, batch_size):\n",
    "    minibatches = get_minibatches(len(train_sentence_list), batch_size)\n",
    "    all_sent = []\n",
    "    for minibatch in minibatches:\n",
    "        mb_sent = [train_sentence_list[t] for t in minibatch]\n",
    "        mb_x, mb_x_len = fill_data(mb_sent)\n",
    "        all_sent.append((mb_x, mb_x_len))           # 添加所需要的batch属性\n",
    "    return all_sent"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 36,
   "metadata": {},
   "outputs": [],
   "source": [
    "train_data = generate(train_sent_list, batch_size)"
   ]
  },
